<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.pd.chatapp.ui.homeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_faq"
        android:name="com.pd.chatapp.ui.FAQFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_faq" />

    <fragment
        android:id="@+id/navigation_history"
        android:name="com.pd.chatapp.ui.historyFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_history" />

    <fragment
        android:id="@+id/navigation_more"
        android:name="com.pd.chatapp.ui.moreFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_more" />
</navigation>